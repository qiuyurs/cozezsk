hildren":["RTeQdvAnEo2JIexvUxlcrh05naf","SWPid3BlNouetYxp7NPcN4Zjnnc"],"parent_id":"Rn63dRPSuo5SxZxP6wVcjtdrnNe"},{"block_id":"RTeQdvAnEo2JIexvUxlcrh05naf","block_type":12,"bullet":{"elements":[{"text_run":{"content":"普通拆包：直接挂上货架（快速但信息少）","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QoQZdvkj4o4m1xxNUG0cVnksnwb"},{"block_id":"SWPid3BlNouetYxp7NPcN4Zjnnc","block_type":12,"bullet":{"elements":[{"text_run":{"content":"精细拆包：要检查衣服成分标 / 熨平褶皱（慢但信息完整）\n","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":" 建议贵重包裹下班前提前送检，避免客人急着查库存时找不到","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"QoQZdvkj4o4m1xxNUG0cVnksnwb"},{"block_id":"Akypd16oKoiax4xJtkicFQjunAf","block_type":5,"heading3":{"elements":[{"text_run":{"content":"分段策略","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Yrxqd4ftlodYi7xaJjGceBOynDg"},{"block_id":"DXYAdmBMiocOvFxPtNScDXC5nPA","block_type":2,"parent_id":"Yrxqd4ftlodYi7xaJjGceBOynDg","text":{"elements":[{"text_run":{"content":"每次上传文档时都可以设置新文档的分段策略，合理的切片策略可以提高检索效率、提升检索结果的准确性。支持选择自动分段或自定义分段。","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}},{"block_id":"MEAOduwnIoNGnwxVIOJcpUxonmc","block_type":4,"heading2":{"elements":[{"text_run":{"content":"常见问题","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}},"parent_id":"Yrxqd4ftlodYi7xaJjGceBOynDg"},{"block_id":"EsKldR6wmoeZqXxNcbEczeNgntf","block_type":5,"heading3":{"elements":[{"text_run":{"content":"